{
  "name": "pure-ui",
  "version": "0.1.9",
  "description": "elegant and lovely components, built with regularjs",
  "main": "dist/pure.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "opn http://127.0.0.1:8000/packages/_example/ && anywhere -p 8000 -s",
    "dev": "concurrent --kill-others \"npm run watch\" \"anywhere\"",
    "watch": "npm run dist -- -w",
    "dist": "webpack --config scripts/webpack.config.js",
    "docs": "node scripts/docs && webpack --config scripts/webpack.config.docs.js",
    "gh-pages": "node scripts/gh-pages",
    "deploy": "npm run docs && npm run gh-pages",
    "patch": "np patch --skip-cleanup",
    "lint": "eslint src src/**/*.rgl",
    "boot": "lerna bootstrap",
    "build": "lerna run build && lerna run example",
    "example": "lerna run example",
    "clean": "lerna clean"
  },
  "eslintConfig": {},
  "author": "fengzilong",
  "license": "MIT",
  "devDependencies": {
    "anywhere": "^1.4.0",
    "autoprefixer": "^6.3.7",
    "autosize": "^3.0.17",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "de-indent": "^1.0.2",
    "director": "^1.2.8",
    "dom-align": "^1.4.0",
    "eslint": "^3.8.1",
    "eslint-config-mo": "latest",
    "eslint-plugin-rgl": "0.0.1",
    "eventemitter2": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "front-matter": "^2.1.0",
    "gh-pages": "^0.11.0",
    "glob": "^7.0.5",
    "html-webpack-plugin": "^2.22.0",
    "is-obj": "^1.0.1",
    "json-loader": "^0.5.4",
    "lerna": "2.0.0-beta.32",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.13.1",
    "marked": "^0.3.5",
    "opn-cli": "^3.1.0",
    "postcss-loader": "^0.9.1",
    "pure-loader": "0.0.1",
    "regular-loader": "^0.1.0",
    "regularjs": "^0.4.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "peerDependencies": {},
  "dependencies": {}
}
